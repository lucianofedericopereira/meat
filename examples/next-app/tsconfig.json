{
  "compilerOptions": {
    "jsx": "react-jsx",
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "Node",
    "strict": true,
    "allowImportingTsExtensions": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "../../..",
    "paths": {
      "@meat": ["src/meat.ts"],
      "@meat/*": ["src/*"]
    }
  },
  "include": ["pages", "components", "declarations.d.ts"]
}
