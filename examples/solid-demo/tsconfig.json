{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "Node",
    "jsx": "react-jsx",
    "jsxImportSource": "solid-js",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowImportingTsExtensions": true,
    "baseUrl": "../../..", 
    "paths": {
      "@meat": ["src/meat.ts"]
    }
  },
  "include": ["src"]
}
