{
  "name": "luciano/meat-laravel",
  "description": "Drop-in Blade-first reactive toolkit powered by MEAT for Laravel",
  "type": "library",
  "require": {
    "php": "^8.1",
    "illuminate/support": "^9.0 || ^10.0 || ^11.0 || ^12.0"  
  },
  "autoload": {
    "psr-4": {
      "Luciano\\MeatLaravel\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Luciano\\MeatLaravel\\MeatServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT"
}
